#include "External_interrupts/External_interrupts.h"

int main(void){
	// External Interrupt0.
	Int0_Init(LOW_LEVEL);
	Enable_Global_Interrupt();
	Int0_Start();

	// External Interrupt1.
	Int1_Init(ANY_LOGICAL);
	Int1_Start();

	// External Interrupt2.
	Int2_Init(RISING_EDGE);
	Int2_Start();

	while(1);

	return 0;
}


